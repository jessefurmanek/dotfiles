# This is Git's per-user configuration file.
[user]
# Please adapt and uncomment the following lines:
name = Jesse Furmanek
	email = jesse.furmanek@gmail.com

[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan

[alias]
	co = checkout
	last = log -1 HEAD
[push]
	default = current
